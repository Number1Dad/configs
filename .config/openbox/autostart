xset fp rehash &


#Adjust trackpoint
xinput --set-prop "SynPS/2 Synaptics TouchPad" "Device Enabled" 0 &
xinput --set-prop "TPPS/2 IBM TrackPoint" "libinput Accel Speed" -0.75 &

